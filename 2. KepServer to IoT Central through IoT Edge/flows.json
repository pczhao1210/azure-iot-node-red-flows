    {
        "id": "ae1b354466ab45a1",
        "type": "function",
        "z": "bbd9966509e94b36",
        "name": "Status Output",
        "func": "var time = new Date().toISOString();\nvar countgood_addr = \"ns=2;s=MES OEE.OEE-CNC1.CountGood\"\nvar countbad_addr = \"ns=2;s=MES OEE.OEE-CNC1.CountBad\";\nvar runningstatus_addr = \"ns=2;s=MES OEE.OEE-CNC1.RunningStatus\";\nvar countgood = msg.payload[countgood_addr];\nvar countbad = msg.payload[countbad_addr];\nvar runningstatus = msg.payload[runningstatus_addr];\nvar msg_interval = msg.payload.msg_inverval;\n\nif (runningstatus == 1)\n    {\n        machine_status = 'On';\n    }\nelse\n    {\n        machine_status= 'Off';\n    }\n\n\nconst status_msg = {\n    'NodeID' : 'CNC1',\n    'RunningStatus' : machine_status,\n    'CountGood' : countgood,\n    'CountBad' : countbad,\n    'Frequency' : msg_interval,\n    'SourceTimestamp' : time\n};\n\nmsg.payload = status_msg;\nreturn msg\n",
        "outputs": 1,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
