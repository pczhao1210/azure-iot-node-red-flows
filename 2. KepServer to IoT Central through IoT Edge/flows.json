    {
        "id": "d45c3f62.8cd78",
        "type": "function",
        "z": "bbd9966509e94b36",
        "name": "Generate values",
        "func": "var power_status = flow.get('power_status');\nvar itemCountGood;\nvar itemCountBad;\n\nif (power_status === undefined)\n{\n    flow.set('power_status', 1);\n}\nelse if (power_status === 1)\n{\n    itemCountGood = parseInt(80.0+Math.round(Math.random()*41));\n    itemCountBad = parseInt(Math.round(Math.random()*11));\n}\nelse\n{\n    itemCountGood = 0.0;\n    itemCountBad = 0.0;\n}\n\nflow.set('item_count_good', itemCountGood);\nflow.set('item_count_bad', itemCountBad);\n\nmsg.payload = [\n    flow.get('power_status'),\n    flow.get('item_count_good'),\n    flow.get('item_count_bad'),\n\n]\nreturn msg;",
        "outputs": 1,
        "noerr": 0,
